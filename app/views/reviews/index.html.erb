<div class="container">
	<h1>レビュー一覧</h1>
	<% @reviews.each do |review| %>
	<%= link_to review_path(review.id) do %>
	<%= attachment_image_tag(review, :image, :fill, 100, 100, fallback: "no_image.jpg") %><br>
	<%= review.title %>
	<% end %>
	<%= review[:created_at].strftime("%Y-%m-%d %H:%M") %>
	<%= review.genre.name %>
	<%= review.user.name %>
	<% end %>
</div>