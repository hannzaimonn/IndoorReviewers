<div class="container text-center">
 <div class="row">
 	<% @all_ranks.each.with_index(1) do |review, i| %>
 	<h4>第<%= i %>位</h4>
 	<%= link_to review_path(review.id) do %>
	<%= attachment_image_tag(review, :image, :fill, 100, 100, fallback: "no_image.jpg") %><br>
	<%= review.title %>
	<% end %>
	<%= review[:created_at].strftime("%Y-%m-%d %H:%M") %>
	<%= review.genre.name %>
	<%= review.user.name %>
 	<% end %>
 </div>
</div>